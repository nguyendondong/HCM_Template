{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:prod": "VITE_USE_EMULATORS=false vite",
    "build": "vite build",
    "build:emulators": "VITE_USE_EMULATORS=true vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "check-env": "node scripts/check-env.js",
    "data:seed": "SEED_TARGET=emulator node scripts/seed-refined.js",
    "data:seed:prod": "SEED_TARGET=production node scripts/seed-refined.js --confirm",
    "data:seed:prod:safe": "SEED_TARGET=production node scripts/seed-refined.js",
    "data:init:dev": "VITE_AUTO_INITIALIZE_DATA=true vite",
    "firebase:init": "firebase init",
    "firebase:login": "firebase login",
    "firebase:emulators": "firebase emulators:start --only auth,firestore,storage --project hcmtemplate",
    "firebase:emulators:ui": "firebase emulators:start --project hcmtemplate",
    "firebase:deploy": "yarn build && firebase deploy",
    "firebase:deploy:hosting": "yarn build && firebase deploy --only hosting",
    "firebase:deploy:rules": "firebase deploy --only firestore:rules,storage",
    "firebase:serve": "firebase serve --only hosting"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.57.4",
    "@types/react-router-dom": "^5.3.3",
    "dotenv": "^17.2.2",
    "firebase": "^12.3.0",
    "firebase-admin": "^13.5.0",
    "framer-motion": "^12.23.13",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hot-toast": "^2.6.0",
    "react-router-dom": "^7.9.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "node-fetch": "^3.3.2",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}
